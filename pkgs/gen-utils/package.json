{
	"name": "gen-utils",
	"version": "0.0.4",
	"license": "UNLICENSED",
	"description": "Shared utilities for generating a passphrase",
	"type": "module",
	"sideEffects": false,
	"exports": {
		".": {
			"import": "./dist/mod.js"
		},
		"./package.json": "./package.json"
	},
	"types": "./src/index.ts",
	"module": "./src/index.ts",
	"scripts": {
		"re:build": "rescript",
		"re:start": "rescript build -w",
		"emit-types": "tsc --emitDeclarationOnly",
		"test": "vitest",
		"test-cov": "vitest --watch false --coverage",
		"fmt": "prettier src --write",
		"make": "esbuild src/index.ts --bundle --minify --platform=browser  --format=esm --outfile=dist/mod.js"
	},
	"devDependencies": {
		"@vitest/coverage-v8": "^0.34.6",
		"esbuild": "^0.19.4",
		"fast-check": "^3.13.0",
		"prettier": "^3.0.3",
		"rescript": "10.1.4",
		"typescript": "^4.9.5",
		"vitest": "^0.34.6"
	}
}

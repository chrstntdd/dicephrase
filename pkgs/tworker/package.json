{
	"name": "@ct/tworker",
	"description": "A utility to make communication with web workers not suck",
	"version": "0.0.1",
	"license": "MIT",
	"type": "module",
	"sideEffects": false,
	"types": "./src/tworker.ts",
	"module": "./dist/tworker.js",
	"files": [
		"package.json",
		"dist",
		"src"
	],
	"exports": {
		".": {
			"import": "./src/tworker.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"fmt": "prettier src --write",
		"make": "esbuild src/tworker.ts --bundle --minify --platform=browser  --format=esm --outfile=dist/tworker.js"
	},
	"devDependencies": {
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"mitt": "^3.0.0"
	}
}

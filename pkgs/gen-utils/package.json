{
  "name": "gen-utils",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "description": "Shared utilities for generating a passphrase",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/gen-utils.mod.js",
      "require": "./dist/gen-utils.cjs"
    },
    "./package.json": "./package.json"
  },
  "browser": "./dist/gen-utils.mod.js",
  "types": "./dist/types/index.d.ts",
  "module": "./dist/gen-utils.mod.js",
  "main": "./dist/gen-utils.cjs",
  "scripts": {
    "re:build": "rescript",
    "re:start": "rescript build -w",
    "emit-types": "tsc --emitDeclarationOnly",
    "test": "vitest",
    "build": "pnpm re:build && node make.js && pnpm emit-types",
    "fmt": "prettier src --write"
  },
  "devDependencies": {
    "gentype": "^4.3.0",
    "rescript": "^9.1.4",
    "vitest": "^0.4.2"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  }
}
